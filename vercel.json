{
  "buildCommand": "npm install && npm run build && cd guest-area && npm install && npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/guest-area",
      "destination": "/guest-area/index.html"
    },
    {
      "source": "/guest-area/(.*)",
      "destination": "/guest-area/$1"
    },
    {
      "source": "/sitemedhome",
      "destination": "/api/sitemedhome"
    },
    {
      "source": "/sitemedhome/(.*)",
      "destination": "/api/sitemedhome?path=$1"
    },
    {
      "source": "/site/:subdomain",
      "destination": "/api/site?subdomain=:subdomain"
    },
    {
      "source": "/site/([^/]+)/(.*)",
      "destination": "/api/site?subdomain=$1&path=$2"
    },
    {
      "source": "/site/([^/]+)",
      "destination": "/api/site?subdomain=$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/index.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/site/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, s-maxage=0, max-age=60"
        },
        {
          "key": "CDN-Cache-Control",
          "value": "no-store"
        },
        {
          "key": "Vercel-CDN-Cache-Control",
          "value": "no-store"
        }
      ]
    }
  ]
}
